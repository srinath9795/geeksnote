{
  "manifest_version": 2,

  "name": "geeksNote",
  "description": "Take Quick Notes to view later",
  "version": "1.0",
  "icons": {
      "19": "icons/notepad_19.png",
      "128":"icons/notepad_128.png",
     "38":"icons/notepad_38.png"
  },
  "permissions": [
        "storage"
    ],
  "background": {
        "persistent": true,
        "scripts": ["background.js"]
    },
  "content_scripts": [
    {
      "matches": ["http://www.geeksforgeeks.org/*","https://www.geeksforgeeks.org/*"],
      "run_at": "document_start",
      "js": ["jquery-1.11.1.min.js","myscript.js"]
    }
  ],
      "page_action": {
          "default_title": "geeksNote",
          "default_icon": {
              "19": "icons/notepad_19.png",
              "38":"icons/notepad_38.png",
              "128":"icons/notepad_128.png"
              
          }
      }
}

