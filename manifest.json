{
  "manifest_version": 2,

  "name": "geeksNote",
  "options_page": "options/notes.html",
  "description": "Geeks way of taking notes.",
  "version": "1.0.2",
  "web_accessible_resources": ["options/notes.html"],
  "icons": {
      "19": "icons/notepad_19.png",
      "128":"icons/notepad_128.png",
     "38":"icons/notepad_38.png"
  },
  "permissions": [
        "storage",
        "tabs"
    ],
  "background": {
        "persistent": true,
        "scripts": ["background.js"]
    },
  "content_scripts": [
    {
      "matches": ["http://www.geeksforgeeks.org/*","https://www.geeksforgeeks.org/*"],
      "run_at": "document_start",
      "js": ["jquery-1.11.1.min.js","myscript.js"]
    }
  ],
      "page_action": {
          "default_title": "geeksNote",
          "default_icon": {
              "19": "icons/notepad_19.png",
              "38":"icons/notepad_38.png",
              "128":"icons/notepad_128.png"
              
          }
      }
}

